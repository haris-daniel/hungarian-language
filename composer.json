{
    "name": "teddhun/hungarian-language",
    "type": "flarum-extension",
    "description": "Extented Hungarian language pack for Flarum",
    "keywords": ["locale"],
    "license": "MIT",
    "require": {
        "flarum/core": "^0.1.0-beta.15"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        },
        "flarum-extension": {
            "title": "Hungarian",
            "icon": {
				"image": "icon.png",
                "backgroundColor": "#00247d",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        },
        "flarum-locale": {
            "code": "hu",
            "title": "Hungarian"
        }
    }
}
